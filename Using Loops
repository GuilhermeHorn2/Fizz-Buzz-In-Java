package project1;

import java.util.Random;
import java.util.Scanner;

public class strt {

	public static void main(String[] args) {
		// Fizz Buzz |
		
		for(int i = 1;i <= 100;i++){
			if(i % 5 == 0 && i % 3 == 0) {
				System.out.println("FizzBuzz");
			}
			if(i % 3 == 0 && i % 5 != 0) {
				// i = 3*k + 0
				System.out.println("Fizz");
			}
			else if(i % 5 == 0 && i % 3 != 0) {
				System.out.println("Buzz");
			}
			else {
				System.out.println(i);
			}
		}
		
		
	}
	}
	
